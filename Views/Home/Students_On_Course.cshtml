@model IEnumerable <Cascadingdropdownlist.Models.Student>

 <div class="jumbotron">

@{
   <table><tr><td>    @Html.ActionLink("Print Attendance Sheet", "Print", "Home", new { id = @ViewBag.CourseID }, new { @class = "btn btn-primary" })
</td></tr></table>
     <table><tr><td>Enrollments on @ViewBag.CourseTitle </td></tr>
        <tr><td>Course Date: @ViewBag.CourseDF  @ViewBag.FromHours @ViewBag.FromMinutes : @ViewBag.ToHours @ViewBag.ToMinutes</td></tr>
           <tr><td> Location:@ViewBag.Venue </td></tr><tr><td> Capacity:@ViewBag.Capacity</td></tr>
     <tr></tr><tr><td>
 </td></tr><tr></tr><tr></tr></table>
}

     
   
<table class="table" id="t01">
    
  
    <tr>
        <th>Attended</th>
        <th>Email Address</th>
        <th>Contact Telephone Number</th>
        <th>
           Last Name
        </th>
        <th>
            First Name
        </th>
        <th></th>
    </tr>


    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Attended, "Absent",new { id = item.ID})
             </td>
            <td>
             <a href="mailto:@item.Email_Address">@item.Email_Address</a> 
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contact_Tel_No)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstMidName)
            </td>
            <td>
                @Html.ActionLink("Certificate", "Certificate", new { id = item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
            </td>
        </tr>
    }
</table>
        </div>