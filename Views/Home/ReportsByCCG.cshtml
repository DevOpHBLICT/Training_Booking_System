@model IEnumerable<Cascadingdropdownlist.ViewModels.ReportsByCCG>


@{
    ViewBag.Title = "ReportsByCCG";
}

<h4>Reports By CCG</h4>


    <div class="jumbotron">

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course_DatesID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Organisation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Course)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Count)
        </th>
       
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Course_DatesID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Organisation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Course)
            
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Count)

        </td>
        
    </tr>
}

</table>
        </div>